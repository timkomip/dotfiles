{
  "vim.useSystemClipboard": true,
  "vim.leader": " ",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "b", "b"],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ]
    },
    {
      "before": ["<leader>", "b", "o"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<leader>", "w", "a"],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closePanel",
        "aichat.close-sidebar"
      ]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "s", "a"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "s", "g"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "H"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "h", "h"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "h", "e"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["vscode-harpoon.gotoEditor1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["vscode-harpoon.gotoEditor2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["vscode-harpoon.gotoEditor3"]
    },
    {
      "before": ["]", "g"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["[", "g"],
      "commands": ["workbench.action.editor.previousChange"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [{ "command": "workbench.action.quickOpen", "args": ["%"] }]
    },
    {
      "before": ["<leader>", "r", "r"],
      "commands": ["workbench.action.tasks.runTask"]
    },
    {
      "before": ["<leader>", "r", "l"],
      "commands": ["workbench.action.tasks.reRunTask"]
    },
    {
      "before": ["<leader>", "t", "e"],
      "commands": [
        { "command": "workbench.action.createTerminalEditor", "args": [] }
      ]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.revealDefinitionAside"]
    }
  ]
}
